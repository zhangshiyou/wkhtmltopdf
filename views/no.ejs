<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <% include ./favicon %>
    <!-- <script src='js/JsBarcode.all.min.js'></script> -->
    <style>
        h1,ul,li{
            font-weight:normal;
            list-style: none;
            padding:0;
            margin:0
        }
        .lf{float:left;}
        .f-fixed:after{content:'\200b';display:block;height:0;clear:both;}
        .f-fixed{*zoom:1;}
        .print{
            text-align: left;
            width:700px;
            margin:0 auto;
            box-sizing: border-box;
            font:14px/1.5 "Microsoft YaHei",tahoma,arial,\5b8b\4f53; color:#303645; outline:0;
        }
        @media print{
            .print-btn-area{
                display: none
            }
            .print{
                min-height:100%
            }
            .no-data{
                display: none;
            }
            .items{
                page-break-before:always;
                page-break-after:always
            }
        }
        .items{
            width:93mm;
            height:32mm;
            display:inline-block;
            box-sizing: border-box;
            margin:2px 0 0 1px;
            padding-bottom: 1px;
            font-size:12px
        }
        .lines{
            border:1px solid #298EE9;
            /* padding-bottom: 2px; */
            box-sizing: border-box
        }
        .height-100{
            height:26px;
        }
        .border-b{
            border-bottom:1px solid #298EE9
        }
        .barcode{
            width:100%;
        }
        .width-50{
            width:50%
        }
        .text.l{
            text-align: left
        }
        .text-r{
            text-align: right
        }
        .text-c{
            text-align: center
        }
        .width-30{
            width:33.33%
        }
        .amrgin-top-10{
            /* margin-top:5px; */
        }
        .font-14{
            font-size:12px
        }
        .pad{
            padding:0 3px;
            box-sizing: border-box
        }
        .width-4{
            width:38%;
        }
        .width-3{
            width:36%;
        }
        .width-2{
            width:26%;
        }
        .width-40{
            width:40%;
        }
        .width-60{
            width:60%;
        }
        .trans svg{
            height:100%;
        }
        .asd svg{
            width:100%; 
            height:100%;
        }
        .asd{
            height:16mm
        }
        .font-big-blod{
            font-size:17px;
            font-weight:bold
        }
        .trans{
            transform: rotate(90deg) translate(41px, 37px);
            -webkit-transform:rotate(90deg) translate(41px, 37px)
        }
        .print-content{
            width:190mm;
            margin:0 auto;
        }
        .over-text{
            width: 100%;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }
    </style>
  </head>
  <body>
    <div>
        <% list.forEach(function(item){%>
            <div class='print-content' v-for='(item,index) in seatList'>
                <h1 class='text-c top-tit'>
                    <!-- {{item.roomName}} -->
                    <%=item.roomName %>
                </h1>
                <div class='f-fixed'>
                    <% item.ticketList.forEach(function(subItem,subIndex){%>
                        <div class='items'>
                            <div class='lines amrgin-top-10 f-fixed'>
                                <div class='lf' style='width:47%;padding:10px 0 0px 5px;box-sizing: border-box'>
                                    <div class='over-text'>
                                        <span class='font-big-blod'><%-subItem.areaName %></span>
                                    </div>
                                    <div>
                                        代码: <span class='font-big-blod'><%-subItem.agentCode %></span>    
                                    </div>
                                    <div>
                                        姓名：<span>
                                            <%=subItem.studentName %>
                                        </span>
                                    </div>
                                    <div>
                                        考场：<span>
                                            <%=subItem.roomName %>
                                        </span>
                                    </div>
                                    <div>
                                        座位：<span>
                                            <%=subItem.seatNo %>
                                        </span>
                                    </div>
                                </div>
                                <div class='lf f-fixed' style='width:53%'>
                                    <div class='lf' style='width:20%;height:32mm'>
                                        <div class='asd trans' style='width:32mm;height:38px'>
                                            <%-subItem.noNo %>
                                        </div>
                                    </div>
                                    <div class='lf' style='width:80%'>
                                        <div class='asd'>
                                            <%-subItem.svgNode %>
                                        </div>
                                        <div class='asd'>
                                            <%-subItem.svgNode %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <% }) %>
                </div>
            </div>
        <% }) %>
    </div>
  </body>
</html>
